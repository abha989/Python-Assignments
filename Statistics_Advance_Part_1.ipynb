    {
      "cell_type": "code",
      "source": [
        "import numpy as np\n",
        "import matplotlib.pyplot as plt\n",
        "\n",
        "def calculate_and_plot_z_scores(data):\n",
        "    \"\"\"\n",
        "    Calculates Z-scores from the input data and plots them.\n",
        "\n",
        "    Parameters:\n",
        "    - data: A list or NumPy array of numerical values.\n",
        "    \"\"\"\n",
        "    # Convert data to NumPy array\n",
        "    data = np.array(data)\n",
        "\n",
        "    # Calculate mean and standard deviation\n",
        "    mean = np.mean(data)\n",
        "    std = np.std(data)\n",
        "\n",
        "    # Avoid division by zero\n",
        "    if std == 0:\n",
        "        raise ValueError(\"Standard deviation is zero. Cannot compute Z-scores.\")\n",
        "\n",
        "    # Calculate Z-scores\n",
        "    z_scores = (data - mean) / std\n",
        "\n",
        "    # Plot histogram of Z-scores\n",
        "    plt.hist(z_scores, bins=30, color='lightgreen', edgecolor='black', alpha=0.7, density=True)\n",
        "    plt.title('Histogram of Z-scores')\n",
        "    plt.xlabel('Z-score')\n",
        "    plt.ylabel('Density')\n",
        "    plt.grid(True)\n",
        "    plt.show()\n",
        "\n",
        "    return z_scores\n",
        "\n",
        "# Example usage:\n",
        "if __name__ == \"__main__\":\n",
        "    # Generate sample data from a normal distribution\n",
        "    sample_data = np.random.normal(loc=100, scale=15, size=1000)\n",
        "\n",
        "    z_scores = calculate_and_plot_z_scores(sample_data)\n"
