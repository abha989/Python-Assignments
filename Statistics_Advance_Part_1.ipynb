        "plt.title('Original Population (Exponential)')\n",
        "plt.xlabel('Value')\n",
        "plt.ylabel('Density')\n",
        "\n",
        "# Histogram of sample means (should appear normal)\n",
        "plt.subplot(1, 2, 2)\n",
        "plt.hist(sample_means, bins=30, color='skyblue', edgecolor='black', density=True)\n",
        "plt.title('Sampling Distribution of the Mean (CLT)')\n",
